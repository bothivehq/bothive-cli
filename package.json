{
  "name": "bothive-cli",
  "description": "CLI for the https://bothive.xyz platform",
  "version": "1.0.0",
  "module": "dist/index.js",
  "type": "module",
  "bin": {
    "bothive": "./bin/bothive.js"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir=./dist --target=node && bun src/build.ts",
    "build:client": "bun run src/generate-client.ts",
    "start": "node dist/cli.js",
    "semantic-release": "semantic-release",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch -m \"Release %s\"",
    "release:minor": "npm version minor -m \"Release %s\"",
    "release:major": "npm version major -m \"Release %s\""
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.49.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.3",
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.0.2",
    "@types/prompts": "^2.4.9",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.2.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hey-api/client-axios": "^0.1.0",
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "npm": "^10.8.2",
    "pino": "^9.3.1",
    "pino-pretty": "^11.2.1",
    "prompts": "^2.4.2",
    "znv": "^0.4.0"
  }
}
